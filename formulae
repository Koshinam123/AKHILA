 \item $(a-b)^2=a^2+b^2-2ab$
 
\end{enumerate}
\begin{enumerate}[label=\Roman*]

 \item $(a+b)^2=a^2+b^2+2ab$
 \item $(a-b)^2=a^2+b^2-2ab$
 
\end{enumerate}
