\begin{itemize}[label=$\star$]

 \item $(a+b)^2=a^2+b^2+2ab$
 \item $(a-b)^2=a^2+b^2-2ab$
 
\end{itemize}
\begin{itemize}[label=$\ast$]

 \item $(a+b)^2=a^2+b^2+2ab$
 \item $(a-b)^2=a^2+b^2-2ab$
 
\end{itemize}
\begin{enumerate}[label=\fbox{\arabic*}]

 \item $(a+b)^2=a^2+b^2+2ab$
 \item $(a-b)^2=a^2+b^2-2ab$
 
\end{enumerate}
\begin{enumerate}[label=\alph*]

 \item $(a+b)^2=a^2+b^2+2ab$
 \item $(a-b)^2=a^2+b^2-2ab$
 
\end{enumerate}

